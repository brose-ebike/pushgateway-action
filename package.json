    "license": "Apache 2.0",
    "bugs": {
        "url": "https://github.com/brose-ebike/pushgateway-action/issues"
    },
    "homepage": "https://github.com/brose-ebike/pushgateway-action#readme",
    "dependencies": {
        "@actions/core": "^1.11.1",
        "@actions/exec": "^1.1.1",
        "@actions/github": "^6.0.1",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/chai": "^5.2.2",
        "@types/expect": "^24.3.2",
        "@types/mocha": "^10.0.10",
        "@types/node": "^22.15.19",
        "@vercel/ncc": "^0.38.3",
        "chai": "^5.2.0",
