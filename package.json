    "license": "Apache 2.0",
    "bugs": {
        "url": "https://github.com/brose-ebike/pushgateway-action/issues"
    },
    "homepage": "https://github.com/brose-ebike/pushgateway-action#readme",
    "dependencies": {
        "@actions/core": "^1.10.0",
        "@actions/exec": "^1.1.1",
        "@actions/github": "^5.1.1",
        "node-fetch": "^3.2.10"
    },
    "devDependencies": {
        "@types/chai": "^4.3.4",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "^15.3.0",
        "@vercel/ncc": "^0.33.3",
        "chai": "^4.3.7",
        "mocha": "^9.2.1",
        "ts-mocha": "^10.0.0",
        "typescript": "^4.8.4"
    }
